properties([parameters([string(description: 'Enter step name', name: 'step_name'), string(description: 'Enter project name', name: 'project_name', trim: true), string(description: 'Enter table name', name: 'table_name')])])
pipeline {
    agent any

    stages {
        stage('build') {
            steps {
                sh '''
				cd python_scripts
                python python get_schema.py "${step_name}" "${project_name}" "${table_name}"
                echo "${step_name}" "${project_name}" "${table_name}"
                
                '''
            }
        }
    }
}
